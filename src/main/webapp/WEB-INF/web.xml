<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

  <display-name>TP3</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>Signup</servlet-name>
    <servlet-class>com.servlet.SignUp</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.servlet.Login</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>com.servlet.Main</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>SearchCep</servlet-name>
    <servlet-class>com.servlet.SearchCep</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SearchCep</servlet-name>
    <url-pattern>/viacep</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Searchly</servlet-name>
    <servlet-class>com.servlet.Searchly</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Searchly</servlet-name>
    <url-pattern>/searchly</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>CoinGecko</servlet-name>
    <servlet-class>com.servlet.CoinGecko</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CoinGecko</servlet-name>
    <url-pattern>/coingecko</url-pattern>
  </servlet-mapping>


  <error-page>
    <error-code>404</error-code>
    <location>/error.jsp</location>
  </error-page>
</web-app>
